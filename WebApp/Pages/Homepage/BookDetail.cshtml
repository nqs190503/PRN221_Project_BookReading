                                                        <a asp-page="/Rate/Rating" asp-route-point="@(i)" asp-route-bookId="@Model.Book.BookId" asp-route-userId="@(int.Parse(Model.UserId==null? "-1": Model.UserId))"><img alt="5" src="~/images/star-off.png"></a>
                                                    }

                                                }

