                                    <div class="rate-story mb-2">
                                        <div class="rate-story__holder" data-score="7.0">
                                            @{
                                                if (ViewBag.RatePoint != null)
                                                {

                                                    for (int i = 1; i <= ViewBag.RatePoint; i++)
                                                    {
                                                        <a asp-page="/Rate/Rating" asp-route-point="@i" asp-route-bookId="@Model.Book.BookId" asp-route-userId="@(int.Parse(Model.UserId==null? "-1": Model.UserId))"><img alt="5" src="~/images/star-on.png"></a>

                                                    }
                                                    for (int i = ViewBag.RatePoint+1; i <= 5; i++)
                                                    {
                                                        <a asp-page="/Rate/Rating" asp-route-point="@(i)" asp-route-bookId="@Model.Book.BookId" asp-route-userId="@(int.Parse(Model.UserId==null? "-1": Model.UserId))"><img alt="5" src="~/images/star-off.png"></a>
                                                    }


                                                }
                                                else
                                                {

                                                    for (int i = 1; i <= 5; i++)
